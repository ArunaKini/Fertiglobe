{
	"name": "DF_RZToCZ_Production_ProductionReportData",
	"properties": {
		"folder": {
			"name": "Production"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_Src_RawZone_Prod_ProductionReportData",
						"type": "DatasetReference"
					},
					"name": "SrcRawZoneProductionManualFile"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DS_Snk_Synapse_Prod_ProductionReportData",
						"type": "DatasetReference"
					},
					"name": "ConformedProductionReportData"
				}
			],
			"transformations": [
				{
					"name": "select1"
				},
				{
					"name": "aggregate1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Date as date,",
				"          Month as date,",
				"          Year as short,",
				"          OpCo as string,",
				"          Plant as string,",
				"          Product as string,",
				"          Downtime as double,",
				"          {Production MTPD} as double,",
				"          {Production MTD} as double,",
				"          {Asset Utilisation} as double,",
				"          {Capacity Efficiency} as double,",
				"          {Onstream Efficiency} as double,",
				"          {Energy Efficiency} as double,",
				"          {Budgeted Production} as double,",
				"          {Budgeted Asset Utilisation} as double,",
				"          {Forecasted Production} as double,",
				"          {Forecasted Asset Utilisation} as double,",
				"          {Production Capacity (MPC)} as short,",
				"          {MTD Downtime} as double",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     modifiedAfter: (addDays(currentUTC(), -1)),",
				"     modifiedBefore: (currentUTC()),",
				"     wildcardPaths:['Production/ProductionReportData/*/ProductionReportData.csv']) ~> SrcRawZoneProductionManualFile",
				"SrcRawZoneProductionManualFile select(mapColumn(",
				"          Date,",
				"          Month,",
				"          Year,",
				"          OpCo,",
				"          Plant,",
				"          Product,",
				"          Downtime,",
				"          Production_MTPD = {Production MTPD},",
				"          Production_MTD = {Production MTD},",
				"          Asset_Utilisation = {Asset Utilisation},",
				"          Capacity_Efficiency = {Capacity Efficiency},",
				"          Onstream_Efficiency = {Onstream Efficiency},",
				"          Energy_Efficiency = {Energy Efficiency},",
				"          Budgeted_Production = {Budgeted Production},",
				"          Budgeted_Asset_Utilisation = {Budgeted Asset Utilisation},",
				"          Forecasted_Production = {Forecasted Production},",
				"          Forecasted_Asset_Utilisation = {Forecasted Asset Utilisation},",
				"          Production_Capacity_MPC = {Production Capacity (MPC)},",
				"          MTD_Downtime = {MTD Downtime}",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 aggregate(groupBy(Date,",
				"          Month,",
				"          Year,",
				"          OpCo,",
				"          Plant,",
				"          Product,",
				"          Downtime,",
				"          Production_MTPD,",
				"          Production_MTD,",
				"          Asset_Utilisation,",
				"          Capacity_Efficiency,",
				"          Onstream_Efficiency,",
				"          Energy_Efficiency,",
				"          Budgeted_Production,",
				"          Budgeted_Asset_Utilisation,",
				"          Forecasted_Production,",
				"          Forecasted_Asset_Utilisation,",
				"          Production_Capacity_MPC,",
				"          MTD_Downtime),",
				"     Cnt = count(1)) ~> aggregate1",
				"aggregate1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          Date as date,",
				"          Month as integer,",
				"          Year as integer,",
				"          OpCo as string,",
				"          Plant as string,",
				"          Product as string,",
				"          Downtime as decimal(25,18),",
				"          Production_MTPD as decimal(25,18),",
				"          Production_MTD as decimal(25,18),",
				"          Asset_Utilisation as decimal(25,18),",
				"          Capacity_Efficiency as decimal(25,18),",
				"          Onstream_Efficiency as decimal(25,18),",
				"          Energy_Efficiency as decimal(25,18),",
				"          Budgeted_Production as decimal(25,18),",
				"          Budgeted_Asset_Utilisation as decimal(25,18),",
				"          Forecasted_Production as decimal(25,18),",
				"          Forecasted_Asset_Utilisation as decimal(25,18),",
				"          Production_Capacity_MPC as decimal(25,18),",
				"          MTD_Downtime as decimal(25,18)",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     truncate:true,",
				"     format: 'table',",
				"     staged: true,",
				"     allowCopyCommand: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          Date,",
				"          Month,",
				"          Year,",
				"          OpCo,",
				"          Plant,",
				"          Product,",
				"          Downtime,",
				"          Production_MTPD,",
				"          Production_MTD,",
				"          Asset_Utilisation,",
				"          Capacity_Efficiency,",
				"          Onstream_Efficiency,",
				"          Energy_Efficiency,",
				"          Budgeted_Production,",
				"          Budgeted_Asset_Utilisation,",
				"          Forecasted_Production,",
				"          Forecasted_Asset_Utilisation,",
				"          Production_Capacity_MPC,",
				"          MTD_Downtime",
				"     )) ~> ConformedProductionReportData"
			]
		}
	}
}